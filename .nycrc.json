{
  "extends": "@istanbuljs/nyc-config-typescript",
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ],
  "extension": [
    ".ts"
  ],
  "include": [
    "src"
  ],
  "exclude": [
    "tests/**/*.spec.ts"
  ],
  "all": true,
  "check-coverage": true,
  "statements": 80,
  "branches": 80,
  "functions": 80,
  "lines": 80
}
