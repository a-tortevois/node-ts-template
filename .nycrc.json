{
  "extends": "@istanbuljs/nyc-config-typescript",
  "report-dir": "coverage",
  "sourceMap": true,
  "all": true,
  "import": [
    "tsx"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "tests/**/*.spec.ts"
  ],
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ],
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
